	"scripts": {
		"build": "gatsby build",
		"clean": "gatsby clean",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"cy:run:dark-mode": "CYPRESS_DARK_MODE=true cypress run",
		"cy:run:component": "cypress run --component",
		"develop": "gatsby develop",
		"lint": "eslint",
		"postinstall": "husky install",
		"serve": "gatsby serve",
		"start": "gatsby serve",
		"test": "jest",
		"test:cypress": "NODE_ENV=development start-server-and-test develop http://localhost:8000 cy:open",
		"test:e2e:ci:light-mode": "start-server-and-test serve http://localhost:9000 cy:run",
		"test:e2e:ci:dark-mode": "start-server-and-test serve http://localhost:9000 cy:run:dark-mode",
		"test:e2e:ci": "npm run test:e2e:ci:light-mode && npm run test:e2e:ci:dark-mode",
		"test:component:ci": "start-server-and-test serve http://localhost:9000 cy:run:component",
		"typecheck": "tsc --noEmit"
	},
